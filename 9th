using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp9
{
    class Program
    {
        static void Main(string[] args)
        {
            int N = 10;
            int topBorder = 1000;
            int bottomBorder = 100;
            int counter = 0;
            int enumiration = 0;
            while (true)
            {
                enumiration += 1;
                int resultChecker = N* enumiration;
                if (resultChecker >= bottomBorder && resultChecker < topBorder)
                {
                    counter += 1;
                }
                if (enumiration == topBorder)
                {
                    Console.WriteLine(counter);
                    break;
                }
            }
        }
    }
}
